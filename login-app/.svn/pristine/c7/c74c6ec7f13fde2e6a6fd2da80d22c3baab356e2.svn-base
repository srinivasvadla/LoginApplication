package w3s.servlet.login;

import java.io.Serializable;

public class Mail implements Serializable {
	public Mail(String mailUniqueId, String mailFrom, String mailContent,
			String mailedDateTime, String subject, String filename) {
		super();
		this.mailUniqueId = mailUniqueId;
		this.mailFrom = mailFrom;
		this.mailContent = mailContent;
		this.mailedDateTime = mailedDateTime;
		this.subject = subject;
		this.filename = filename;
		
	}



	private String mailUniqueId;
	private String mailFrom;
	private String mailContent;
	private String mailedDateTime;
	private String subject;
	private String filename;

	


	public String getFilename() {
		return filename;
	}


	public void setFilename(String filename) {
		this.filename = filename;
	}



	public Mail() {
	}

	

	



	public String getMailFrom() {
		return mailFrom;
	}

	public void setMailFrom(String mailFrom) {
		this.mailFrom = mailFrom;
	}

	public String getMailContent() {
		return mailContent;
	}

	public void setMailContent(String mailContent) {
		this.mailContent = mailContent;
	}

	public String getMailedDateTime() {
		return mailedDateTime;
	}

	public void setMailedDateTime(String mailedDateTime) {
		this.mailedDateTime = mailedDateTime;
	}

	public String getMailUniqueId() {
		return mailUniqueId;
	}

	public void setMailUniqueId(String mailUniqueId) {
		this.mailUniqueId = mailUniqueId;
	}
	
	

	public String getSubject() {
		return subject;
	}



	public void setSubject(String subject) {
		this.subject = subject;
	}



	@Override
	public String toString() {
		return "Mail [mailFrom=" + mailFrom + ", mailContent=" + mailContent
				+ ", mailedDateTime=" + mailedDateTime + "]";
	}

}
